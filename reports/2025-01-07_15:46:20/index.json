                    "        pm.test(\"Requset is 200\", function () {",
                    "    pm.response.to.have.status(200);",
                    "});",
                    "",
                    "pm.test(\"Content-Type is text/html\", function () {",
                    "    pm.expect(pm.response.headers.get('Content-Type'))",
                    "        .to.match(/^text\\/html.*charset=UTF-8/i);",
                    "});",
                    "",
                    "const rx = /\\s*value\\s*:\\s*['\"]([0-9a-f]{15,513})['\"]/;",
                    "",
                    "const body = pm.response.text();",
                    "",
                    "const match = body.match(rx);",
                    "",
                    "if (match && match[1]) {",
                    "    pm.environment.set(\"csrf\", match[1]);    ",
                    "    console.log(\"CSRF value:\", match[1]);",
                    "} else {",
                    "    throw new Error(\"Regex extraction failed: No CSRF match found\");",
                    "}",
                    "",
                    "pm.test(\"CSRF extracted successfully\", function () {",
                    "    pm.expect(pm.environment.get(\"csrf\")).to.not.be.undefined;",
                    "});",
                    "      "
                  ]
                }
              }
